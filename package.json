{
  "name": "pichronos",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.2",
    "cron": "^1.3.0",
    "express": "^4.16.2",
    "husky": "^0.14.3",
    "lowdb": "^1.0.0",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.2.0",
    "react-modal": "^3.1.11",
    "react-scripts": "1.0.17",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node server",
    "dev:server": "nodemon server",
    "dev:client": "react-scripts start",
    "dev": "npm-run-all -p dev:server dev:client",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettify": "prettier --write ./{src,server}/**/*",
    "precommit": "npm run prettify"
  },
  "devDependencies": {
    "nodemon": "^1.14.11",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2"
  },
  "nodemonConfig": {
    "ignore": [
      "db.json"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always"
  }
}
